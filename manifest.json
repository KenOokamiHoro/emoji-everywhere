{

  "manifest_version": 2,
  "name": "notoify",
  "version": "1.0",

  "description": "Display Noto Color Emoji as png.",

  "icons": {
    "128": "icons/icon.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["notoify.js"],
      "css": ["emoji.css"]
    }
  ],
  "web_accessible_resources": [
    "assets/png/*.png",
    "assets/svg/*.svg"
  ]

}
